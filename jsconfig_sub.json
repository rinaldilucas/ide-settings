{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"reactorCmps/*": ["../../*"],
			"reactor2/*": ["../../../reactor2/*"]
		},
		"jsx": "react",
		"module": "ES6",
		"target": "ES6",
		"allowJs": true,
		"checkJs": true,
		"moduleResolution": "node"
	},
	"include": [
		"./**/*",
		"../../../../*.d.ts"
	],
	"exclude": [
		"../../../../node_modules/**/*",
		"../../node_modules/**/*"
	]
}
