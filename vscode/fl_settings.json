  "debug.onTaskErrors": "abort",
  "diffEditor.ignoreTrimWhitespace": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.foldingImportsByDefault": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": "'zero', 'cv27', 'cv30', 'cv31', 'ss03', 'ss04', 'ss05'",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "emmet.triggerExpansionOnTab": true,
  "eslint.validate": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "json5"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.associations": {
    "*.hbs": "handlebars",
    "*.md": "markdown",
    "*html": "html"
  },
  "files.exclude": {
    "**/.angular": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.csproj": true,
    "**/*.sln": true,
    "**/bin": true,
    "**/CVS": true,
    "**/dist": true,
    "**/obj": true,
    "node_modules": true,
    "qex-dev-lib": true,
    ".nx": true
  },
  "files.watcherExclude": {
    "**/.angular": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.ignoreLimitWarning": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": true,
    "scminput": false,
    "yaml": true
  },
  "gitlens.defaultDateFormat": "D/MM/YYYY, HH:m",
  "gitlens.defaultDateLocale": null,
  "gitlens.defaultDateShortFormat": "D/MM/YY",
  "gitlens.defaultTimeFormat": "HH:m",
  "gitlens.views.stashes.files.layout": "list",
  "i18n-ally.displayLanguage": "pt-br",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["src/assets/i18n"],
  "i18n-ally.sourceLanguage": "en",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveServer.settings.donotShowInfoMsg": true,
  "material-icon-theme.activeIconPack": "angular_ngrx",
  "php-imports.formatOnSave": true,
  "php-imports.print.wrap.all": false,
  "php-imports.print.wrap.limit": false,
  "php-imports.unused.enable": true,
  "search.exclude": {
    ".angular": true,
    "**/.angular": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/flc-dev-lib": true,
    "**/node_modules": true,
    "**/qex-dev-lib": true,
    "**/vendor": true,
    "**/web.config": true,
    "**/yarn.lock": true,
    "package-lock.json": true,
    "src/assets/pdfjs/**": true
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "settingsSync.ignoredExtensions": [],
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.env.windows": {},
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "statusBar.debuggingBackground": "#263238",
    "statusBar.noFolderBackground": "#212121"
  },
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "none",
  "editor.linkedEditing": true,
  "window.zoomPerWindow": false,
  "window.zoomLevel": 1,
  "chat.commandCenter.enabled": false
}
